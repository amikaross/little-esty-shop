<style>
* {
  box-sizing: border-box;
}

.column {
  float: left;
  width: 50%;
  padding: 10px;
}

.row:after {
  content: "";
  display: table;
  clear: both;
}
</style>
<body>
<%= render partial: "shared/header", locals: { subtitle: @merchant.name } %>
<h3 style="text-align:center;background-color:whitesmoke;border-top-style:solid">My Items</h3>
<div class="row">
  <div class="column">
    <h2>Enabled Items</h2>
    <p>Some text..</p>
    <h2>Disabled Items</h2>
  </div>
  <div class="column">
    <h2>Top Items</h2>
    <p>Some text..</p>
  </div>
</div>
</body>